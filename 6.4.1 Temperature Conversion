#include <iostream>
using namespace std;


double CelsiusToFahrenheit(double tempC){
   double tempF = (9.0/5.0) * tempC + 32;
   return tempF;
}


int main() {
   double tempF;
   double tempC;

   cout << "Enter temperature in Celsius: " << endl;
   cin >> tempC;

   tempF = CelsiusToFahrenheit(tempC);

   cout << "Fahrenheit: " << tempF; 

   return 0;
}
